{
  "name": "pass-branch-pr-to-draft",
  "version": "1.0.1",
  "description": "Pass all the associated open PR's to draft",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clovis-team/pass-branch-pr-to-draft.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Deployment"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Clovis-team/pass-branch-pr-to-draft/issues"
  },
  "homepage": "https://github.com/Clovis-team/pass-branch-pr-to-draft#readme",
  "dependencies": {},
  "devDependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@typescript-eslint/parser": "^5.12.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "flow-bin": "^0.172.0",
    "graphql": "16.3.0",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}